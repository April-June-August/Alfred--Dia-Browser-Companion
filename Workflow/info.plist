				<key>script</key>
				<string>function run(argv) {
    // Check if this is an error argument
    if (argv[0] == "error") {
        return;
    }

    // Get a reference to the Dia application
    var dia = Application('Dia');

    // Parse the arguments for window and tab indices
    var windowIndex = argv[0];
    var tabIndex = argv[1];

    // Focus the tab (this automatically brings the window forward)
    dia.windows[windowIndex].tabs[tabIndex].focus();

    return;
}</string>
